var enemyByEGround = EnemyByEGround(self);
var enemyViewIn = EnemyViewIn();
if (
(collision_rectangle(x-sprite_width/3+_xspd,y-sprite_height/2 - 4 + (_yspd),x+sprite_width/3+_xspd,y-sprite_height/2 - 4,O_Block_Parent,1,0) &amp;&amp; _yspd &lt; 0)
