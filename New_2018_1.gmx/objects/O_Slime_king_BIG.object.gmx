|| (collision_rectangle(x-sprite_width/3+_xspd*2,y-sprite_height/2 - 4 + (_yspd),x+sprite_width/3+_xspd*2,y,O_Block_Parent,1,0) &amp;&amp; _yspd &gt; 1)
{
y -= _yspd;
_yspd *= -1;
}
else if collision_rectangle(x-sprite_width/2 - _xspd,y,x+sprite_width/2 - _xspd,y+sprite_height/2 - 4+_yspd,O_Block_Parent,1,0)
|| (collision_rectangle(x-sprite_width/2 - _xspd ,y,x+sprite_width/2 - _xspd,y+sprite_height/2 - 4+_yspd,O_Block_empty_parent,1,0) &amp;&amp; _yspd &gt; 0)
{
_yspd = 0;
}else{
y += _yspd;
}

if (collision_rectangle(x-sprite_width/2+_xspd*2,y,x+sprite_width/2+_xspd*2,y+sprite_height/2 + 2,O_Block_empty_parent,1,0))
&amp;&amp; (enemyByEGround
|| !(collision_rectangle(x-sprite_width/3+_xspd,y,x-sprite_width/3+_xspd,y+sprite_height/2 + 1,O_Block_empty_parent,1,0))
